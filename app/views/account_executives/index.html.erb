<p id="notice"><%= notice %></p>

<h1>Account Executives</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Carrier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @account_executives.each do |account_executive| %>
      <tr>
        <td><%= account_executive.firstname %></td>
        <td><%= account_executive.lastname %></td>
        <td><%= account_executive.carrier %></td>
        <td><%= link_to 'Show', account_executive %></td>
        <td><%= link_to 'Edit', edit_account_executive_path(account_executive) %></td>
        <td><%= link_to 'Destroy', account_executive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Account Executive', new_account_executive_path %>
